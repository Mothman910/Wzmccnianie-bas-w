{
  "manifest_version": 3,
  "name": "Wzmacnianie bas√≥w",
  "version": "1.0",
  "description": "Extension to count header elements on inactive tabs and display them on active tab.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Icons/led.png",
      "48": "Icons/led.png",
      "128": "Icons/led.png"
    }
  },
  "icons": {
    "16": "Icons/led.png",
    "48": "Icons/led.png",
    "128": "Icons/led.png"
  },
  "permissions": ["tabs", "activeTab", "scripting", "storage"],

  "host_permissions": ["http://*/*", "https://*/*"]
}
